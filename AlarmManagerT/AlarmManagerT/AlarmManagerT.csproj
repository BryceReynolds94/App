<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\group_default.svg" />
    <None Remove="Images\icon_about.svg" />
    <None Remove="Images\icon_add.svg" />
    <None Remove="Images\icon_alert.svg" />
    <None Remove="Images\icon_alert_off.svg" />
    <None Remove="Images\icon_alert_snooze.svg" />
    <None Remove="Images\icon_alert_snooze_inactive.svg" />
    <None Remove="Images\icon_alert_snooze_off.svg" />
    <None Remove="Images\icon_clear.svg" />
    <None Remove="Images\icon_confirm.svg" />
    <None Remove="Images\icon_delete.svg" />
    <None Remove="Images\icon_history.svg" />
    <None Remove="Images\icon_settings.svg" />
    <None Remove="Images\icon_share.svg" />
    <None Remove="Images\icon_trigger.svg" />
    <None Remove="Images\launcher_icon.svg" />
    <None Remove="Images\user_default.svg" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Images\group_default.svg" />
    <EmbeddedResource Include="Images\icon_about.svg" />
    <EmbeddedResource Include="Images\icon_add.svg" />
    <EmbeddedResource Include="Images\icon_alert.svg" />
    <EmbeddedResource Include="Images\icon_alert_off.svg" />
    <EmbeddedResource Include="Images\icon_alert_snooze.svg" />
    <EmbeddedResource Include="Images\icon_alert_snooze_inactive.svg" />
    <EmbeddedResource Include="Images\icon_alert_snooze_off.svg" />
    <EmbeddedResource Include="Images\icon_clear.svg" />
    <EmbeddedResource Include="Images\icon_confirm.svg" />
    <EmbeddedResource Include="Images\icon_delete.svg" />
    <EmbeddedResource Include="Images\icon_history.svg" />
    <EmbeddedResource Include="Images\icon_settings.svg" />
    <EmbeddedResource Include="Images\icon_share.svg" />
    <EmbeddedResource Include="Images\icon_trigger.svg" />
    <EmbeddedResource Include="Images\launcher_icon.svg" />
    <EmbeddedResource Include="Images\user_default.svg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NLog" Version="4.7.5" />
    <PackageReference Include="Plugin.FirebasePushNotification" Version="3.3.10" />
    <PackageReference Include="TLSharp.Standard" Version="1.0.1" />
    <PackageReference Include="Xamarin.FFImageLoading.Svg.Forms" Version="2.4.11.982" />
    <PackageReference Include="Xamarin.Forms" Version="4.8.0.1560" />  
    <PackageReference Include="Xamarin.Essentials" Version="1.5.3.2" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\AppResources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>AppResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\AlertPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\ConfigureGroupPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\ConfigureKeywordPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\LoginPhonePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\LoginCodePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\AccountPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\AppResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AppResources.resx</DependentUpon>
    </Compile>
    <Compile Update="Views\HomeStatusPage.xaml.cs">
      <DependentUpon>HomeStatusPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\AccountPage.xaml.cs">
      <DependentUpon>AccountPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\LoginCodePage.xaml.cs">
      <DependentUpon>LoginCodePage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\LoginPhonePage.xaml.cs">
      <DependentUpon>LoginPhonePage.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>